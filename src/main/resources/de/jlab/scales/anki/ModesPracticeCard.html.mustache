<div class="card mb-3">
	<h4 class="card-header">
	  Play {{model.modeName}} in {{model.direction}} Direction
	</h4>
	<div class="card-body">
		<img class="card-img-top" src="{{pngName}}">
		<p class="card-text">
			<audio controls src="{{mp3Name}}"></audio>
		</p>
		<p class="card-text">Tags: {{tags}}</p>
	</div>
</div>
